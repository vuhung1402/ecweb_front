// jsconfig.json
{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "@core/*": [
                "src/core/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@images/*": [
                "src/assets/images/*"
            ],
            "@component/*": [
                "src/component/*"
            ],
            "@icon/*": [
                "src/assets/icon/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@redux/*":[
                "src/redux/*"
            ],
            "@api/*":[
                "src/api/*"
            ],
            "@hooks/*":[
                "src/hooks/*"
            ],
            "@constants/*":[
                "src/constants/*"
            ],
            "@store/*":[
                "src/store/*"
            ]
        }
    },
}